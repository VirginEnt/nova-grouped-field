{
    "name": "virginent/nova-grouped-field",
    "description": "Combine Nova fields into one output",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": ">=8.1.0"
    },
    "autoload": {
        "psr-4": {
        "VirginEnt\\NovaGroupedField\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "VirginEnt\\NovaGroupedField\\FieldServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
